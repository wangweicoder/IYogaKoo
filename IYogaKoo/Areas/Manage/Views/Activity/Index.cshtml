@model IEnumerable<IYogaKoo.ViewModel.ViewClass>

@{
    ViewBag.Title = "活动管理";
    Layout = "~/Areas/Manage/Views/Shared/_Master.cshtml";
    }

<div class="content-box">
    <div class="content-box-header">
        <h3>活动管理</h3>
    </div>
    <div class="content-box-content">
        <div class="notification attention png_bg">
            <a href="#" class="close"><img src="~/Content/Back/images/cross_grey_small.png" title="Close this notification" alt="close"></a>
            <div>
                This is a Content Box. You can put whatever you want in it. By the way, you can close this notification with the top-right cross.
            </div>
        </div>
        <table>
            <thead>
                <tr>
                    <td>编号</td>
                    <td>名称</td>
                    <td>主题</td>
                    <td>时间</td>
                    <td>地区</td>
                    <td>费用</td>
                    <td>状态</td>
                    <td>浏览量</td>
                    <td>发起时间</td>
                    <td></td>
                </tr>
            </thead>
            <tbody>
               
            </tbody>
        </table>
    </div>
</div>
@section scripts{
<script src="~/Scripts/laypage/laypage-admin.js"></script>
    }