                    if (data.code == 0) {
                        layer.msg("邮箱修改成功！");
                        window.location = '/Login/UpdatePwd';
                        $("#NewUEmail").val("");
                        $("#NewUEmail2").val("");
                    } else {
                        layer.msg("邮箱修改失败！");
                    }

                }, error: function () {
                    layer.msg("邮箱修改失败");
                }
            });
        }

    });

    function checkIdTypeNull()
    {
        var IdType = $("#IdType").val();
        var IdCardNum = $("#IdCardNum").val();
        var bl =false;
        if (IdType != "0" && IdCardNum != "")
        {
            bl = true;
        }
        return bl;
    }
    $('#btnIdType').click(function () {
        var IdType = $("#IdType").val();
        var IdCardNum = $("#IdCardNum").val();

        if (checkIdTypeNull()) {
            $.ajax({
                url: '/Login/UpdateIdType?t=' + Math.random(),
                type: 'POST',
                data: {
                    IdType: IdType,
