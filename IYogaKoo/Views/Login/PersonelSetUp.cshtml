                    自我级别评定&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    <label>级别</label>  
                    @{
                          if (Model.Ulevel == null || Model.Ulevel == 0)
                          {
                            @:基础习练者
                }

                          if (Model.Ulevel == 1)
